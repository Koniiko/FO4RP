#define __MODULE_SUI__
#include "sui.fosh"

#ifdef __CLIENT
#include "sui_input.fosh"
#include "sui_input_wasd.fosm"
#endif

class ConfiguredModuleSUI : ModuleSUI 
{
    bool Start() override {
        ModuleSUI::Start();

        #ifdef __CLIENT
        /// Register hotkeys
        WASDHotkeyActionSUI(this);
        #endif

        return true;
    }
}

ModuleSUI@ module = ConfiguredModuleSUI();

ModuleSUI@ get_SUI() {
    return module;
}