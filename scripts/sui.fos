#define __MODULE_SUI__
#include "sui.fosh"

#ifdef __CLIENT
#include "sui_input.fosh"
#include "sui_input_wasd.fosm"
#endif

class CustomModuleSUI : ModuleSUI 
{
    bool Start() override {
        ModuleSUI::Start();

        #ifdef __CLIENT
        /// Register hotkeys
        this.Hotkeys.AddHotkeyAction(WASDHotkeyActionSUI());
        #endif

        return true;
    }
}

CustomModuleSUI@ module = CustomModuleSUI();

ModuleSUI@ get_SUI() {
    return module;
}